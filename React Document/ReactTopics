#spread attribute: passes two props to the component
const props = {a: 1 , b: 2};
const element = <Sum {...props}/>; //passing two props

#STATES:
const [houses, setHouses] = useState(houseArray;)

const addHouse = () => {
  setHouses([
    ...houses, 
    {
      id: 3,
      address: "Lewisville TX"
    }
  ]);
};

<button onClick={addHouse}>
